{
    "name": "TeX Live",
    "dockerFile": "Dockerfile",
    "runArgs": ["--name", "texlive_devcontainer_${localEnv:USER}"],
    "customizations": {
        "vscode": {
            "extensions": ["james-yu.latex-workshop"],
            "settings": {
                "latex-workshop.formatting.latex": "latexindent",
                "latex-workshop.latex.outDir": "./out"
            }
        }
    }
}